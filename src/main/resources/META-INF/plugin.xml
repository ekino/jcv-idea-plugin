<idea-plugin>
    <id>com.ekino.oss.jcv-idea-plugin</id>
    <name>JCV</name>
    <version>XXX</version>
    <vendor>ekino</vendor>

    <description><![CDATA[ Description will be added by gradle build]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="183.0" />

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplatesProvider implementation="com.ekino.oss.jcv.idea_plugin.autocomplete.JcvTemplateProvider" />
        <liveTemplateMacro implementation="com.ekino.oss.jcv.idea_plugin.autocomplete.macro.DateFormatMacro" />
        <liveTemplateMacro implementation="com.ekino.oss.jcv.idea_plugin.autocomplete.macro.LanguagesMacro" />
    </extensions>

    <change-notes><![CDATA[	Change notes will be filled by gradle build ]]>
    </change-notes>

</idea-plugin>
