<idea-plugin>
    <id>com.ekino.oss.jcv-idea-plugin</id>
    <name>JCV</name>
    <version>XXX</version>
    <vendor>ekino</vendor>

    <description><![CDATA[ Description will be added by gradle build]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="201.6668"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <annotator language="JSON"
                   implementationClass="com.ekino.oss.jcv.idea_plugin.annotator.JcvValidatorAnnotator"
        />
        <completion.contributor language="JSON"
                                implementationClass="com.ekino.oss.jcv.idea_plugin.autocomplete.JcvValidatorContributor"/>
    </extensions>

    <change-notes><![CDATA[	Change notes will be filled by gradle build ]]></change-notes>

</idea-plugin>
